<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationTest">
			  <xpath>*/ThingDef[*]/thingClass/text()[.="Pawn"]</xpath>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>*/ThingDef[*]/thingClass</xpath>
				<value>
					<thingClass>Psychology.PsychologyPawn</thingClass>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>*/ThingDef[*]/inspectorTabs</xpath>
				<value>
					<li>Psychology.ITab_Pawn_Psyche</li>
				</value>
			</li>
		</operations>
	</Operation>
	
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationTest">
			  <xpath>*/ThingDef[*]/race/intelligence/text()[.="Humanlike"]</xpath>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>*/ThingDef[*]/recipes</xpath>
				<value>
					<li>TreatPyromania</li>
					<li>TreatChemicalInterest</li>
					<li>TreatChemicalFascination</li>
					<li>TreatDepression</li>
				</value>
			</li>
		</operations>
	</Operation>
	
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationTest">
			  <xpath>*/ThingDef[*]/race/hediffGiverSets/li/text()[.="OrganicStandard"]</xpath>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>*/ThingDef[*]/race/hediffGiverSets</xpath>
				<value>
					<li>OrganicPsychology</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>